{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "plugin:prettier-recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "plugins": ["jsx-a11y"],
  "rules": {
    "semi": "error"
  },
  "ignorePatterns": ["webpack/*", "node_modules/*"]
}
